using System;
using System.Linq;

namespace _02.Knights_of_Honor
{
    class Program
    {
        static void Main(string[] args)
        {
            Action<string> names = (name) => { Console.WriteLine(name); };

            Console.ReadLine()
                .Split(" ")
                .ToList()
            .ForEach(name => Console.WriteLine(("Sir " + name)));
           
        }
    }
}
